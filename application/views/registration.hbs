	<header>
		<h1>Registration Form</h1>
	</header>
	<div id="error"></div>
	<form id="register" action="register">
		<label for="username"><b>Username:</b></label>
		<input type="text" id="username" name="username" />
		
		<label for="email"><b>Email:</b></label>
		<input type="email" id="email" name="email" required />
		
		<label for="password"><b>Password:</b></label>
		<input type="password" id="password" name="password"/>
		
		<br>
		<label for="redo"><b>Confirm Password:</b></label>
		<input type="password" id="redo" name="redo"/>
		
		<br>
		<label for="age">Are you 13+ years of age?</label>
		<p>
		<input type="radio" id="ageyes" name="age" value="yes" required/>Yes
		<br>
		<input type="radio" id="ageno" name="age" value="no" />No
		</p>
		
		<br>
		<a href="TOS and Privacy Rules">
			<input type="checkbox" id="tos" name="tos" required>
			<label for="tos">TOS and Privacy Rules</label>
		</a>
		<button type="submit">Register</button>
	</form>
	<footer>
	</footer>

